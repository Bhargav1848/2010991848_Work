<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script type="module" src="milsymbol-2.0.0/src/milsymbol.js"></script>
    <title>Document</title>
</head>

<body>
    <div id="main" style="width:50%;margin:auto">
        <table cellspacing="30px">
            <tr>
                <th>S.No</th>
                <th>Description</th>
                <th>Assigned On</th>
                <th>Deadline</th>
                <th>Status</th>
                <th>Edit</th>
                <th>Delete</th>
            </tr>

            <% for(var i=0;i< list.length;i++) {%>
                <tr>
                    <td>
                        <%=i+1%>
                    </td>
                    <td>
                        <%=list[i].description%>
                    </td>
                    <td>
                        <%=list[i].time%>
                    </td>
                    <td>
                        <%=list[i].deadline%>
                    </td>
                    <td>
                        <input type="checkbox" checked="<%=list[i].id%>" />
                    </td>
                    <td>
                        <a href="/edit/ <%=list[i].id%>">
                            <button>Edit</button
                        </a>
                    </td>
                    <td>
                        <a href="/delete/ <%=list[i].id%>">
                            <button>Delete</button>
                        </a>
                    </td>
                </tr>

                <%}%>
        </table>
    </div>
</body>

</html>